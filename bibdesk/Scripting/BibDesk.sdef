                <cocoa key="papersFolder"/>
            </property>
            <property name="all typenames" code="atyp" description="All the known BibTeX publication types." access="r"><type type="text" list="yes"/>
                <cocoa key="allTypes"/>
            </property>
            <property name="all fieldnames" code="afnm" description="All the known BibTeX field names" access="r"><type type="text" list="yes"/>
