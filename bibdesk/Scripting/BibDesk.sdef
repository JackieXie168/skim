            </property>  
            <property name="BibTeX string" code="BTeX" type="text" description="String for BibTeX entry. Can be set only at or immediately after initialization.">
                <cocoa key="bibTeXString"/>
            </property>
            <property name="RIS string" code="RISs" type="text" access="r" description="RIS representation of the publication record.">
                <cocoa key="risString"/>
            </property>
            <property name="document" code="docu" type="document" access="r" description="The bibliography containing this publication record.">
                <cocoa key="document"/>
            </property>  
            <responds-to name="show">
                <cocoa method=""/>
            </responds-to>
        </class>
        
