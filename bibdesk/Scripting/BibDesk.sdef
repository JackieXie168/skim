            </responds-to>
            <responds-to name="search">
                <cocoa method=""/>
            </responds-to>
        </class>
            
        <class name="publication" plural="publications" code="bibi" inherits="item" description="A publication.">
            <cocoa class="BibItem"/>
            <element type="author" name="author" description="Authors of the publication.">
                <cocoa key="pubAuthors"/>
            </element>
            <element type="field" description="The fields of the publication. These should be accessed by name.">
                <cocoa key="bibFields"/>
            </element>
            <property name="cite key" code="ckey" type="text" description="Key for citing the publication.">
