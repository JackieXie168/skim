  { echo "$as_me:$LINENO: WARNING: Couldn't find X / Motif -- you will be able to compile
        pdftops, pdftotext, pdfinfo, pdffonts, and pdfimages, but not xpdf" >&5
echo "$as_me: WARNING: Couldn't find X / Motif -- you will be able to compile
        pdftops, pdftotext, pdfinfo, pdffonts, and pdfimages, but not xpdf" >&2;}
fi
