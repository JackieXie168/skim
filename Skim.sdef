    <suite name="Skim Suite" code="SKIM"
            description="Skim specific commands and classes for dealing with PDF.">

        <cocoa name="SkimSuite"/>

        <command name="revert" code="SKIMRvrt"
            description="Revert a document. Don't use this when you have turned on the auto-relead functionality.">
            <direct-parameter type="specifier"
                description="The document(s) or window(s) to revert."/>
        </command>

        <command name="select" code="SKIMselL"
            description="Select text or notes in a document.">
            <cocoa class="SKSelectCommand"/>
            <direct-parameter
                description="The character range(s) or note(s) to select.">
                <type type="specifier" list="yes"/>
                <type type="specifier"/>
            </direct-parameter>
			<parameter name="animating" code="Anim" type="boolean" optional="yes"
				description="Animate when selecting?"> 
				<cocoa key="Animate"/> 
			</parameter>
        </command>

        <command name="go" code="SKIMGoTo"
            description="Go to a location.">
            <direct-parameter type="document"
                description="The document in which to scroll."/>
            <parameter name="to" code="to  "
                description="The page, note, TeX line, or selection to go to.">
                <type type="page"/>
                <type type="note"/>
                <type type="TeX line"/>
                <type type="specifier" list="yes"/>
                <type type="specifier"/>
                <cocoa key="To"/>
            </parameter>
			<parameter name="from" code="from" type="file" optional="yes" 
				description="The TeX source file. By default this is derived from the file. Only applies for a TeX line."> 
				<cocoa key="Source"/> 
			</parameter>
			<parameter name="showing reading bar" code="RBar" type="boolean" optional="yes" 
				description="Whether to move the reading bar instead of selecting. Only applies for a TeX line."> 
				<cocoa key="ShowReadingBar"/> 
			</parameter>
        </command>

        <command name="convert notes" code="SKIMConv"
            description="Convert in a document PDF annotations to Skim notes.">
            <direct-parameter type="document"
                description="The document(s) for which to convert notes."/>
            <parameter name="waiting" code="Wait" type="boolean" optional="yes"
                description="Whether to wait for the conversion to finish. Default is true">
                <cocoa key="Wait"/>
            </parameter>
        </command>

        <command name="read notes" code="SKIMRNot"
            description="Read notes from a Skim document and ad them to the PDF document.">
            <direct-parameter type="document"
                description="The document(s) for which to read notes."/>
			<parameter name="from" code="from" type="file" 
				description="The file from which to read the notes."> 
				<cocoa key="File"/> 
